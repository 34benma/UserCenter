<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="cn.wantedonline.usercenter.dao.UserInfoMapper">
    <insert id="insertNewUser" flushCache="true" parameterType="cn.wantedonline.usercenter.domain.UserInfoPo">
        INSERT INTO userinfo
        <if test="id != null">
            SET id = #{id}
        </if>
        <if test="uid != null">
            SET uid = #{uid}
        </if>
        <if test="userName != null">
            SET username = #{userName}
        </if>
        <if test="gender != null">
            SET gender = #{gender}
        </if>
        <if test="password != null">
            SET password = #{password}
        </if>
        <if test="salt != null">
            SET salt = #{salt}
        </if>
        <if test="tel != null">
            SET tel = #{tel}
        </if>
        <if test="email != null">
            SET email = #{email}
        </if>
        <if test="lFrom != null">
            SET l_from = #{lFrom}
        </if>
        <if test="createTime != null">
            SET create_time = #{createTime}
        </if>
        <if test="updateTime != null">
            SET update_time = #{updateTime}
        </if>
    </insert>
</mapper>